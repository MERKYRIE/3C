dotnet_diagnostic.UNT0002.severity = silent
dotnet_diagnostic.UNT0017.severity = silent
dotnet_diagnostic.UNT0018.severity = silent
dotnet_diagnostic.UNT0019.severity = silent
dotnet_diagnostic.UNT0022.severity = silent
dotnet_diagnostic.UNT0024.severity = silent
dotnet_diagnostic.UNT0026.severity = silent
dotnet_diagnostic.UNT0028.severity = silent
dotnet_diagnostic.UNT0032.severity = silent
dotnet_diagnostic.UNT0016.severity = silent
dotnet_diagnostic.UNT0003.severity = silent
dotnet_diagnostic.UNT0006.severity = silent
dotnet_diagnostic.UNT0010.severity = silent
